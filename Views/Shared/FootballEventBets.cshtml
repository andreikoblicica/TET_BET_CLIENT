@*     { *@
@*         <h4>@bets.ElementAt(i).betTypeName</h4> *@
@*         <div class="row"> *@
@*           @for (var j = 0; j < @bets.ElementAt(i).betsList.Count; j++) *@
@*           { *@
@*               <div class="col"> *@
@*                   <button id="" type="button" class="btn btn-info btn-lg btn-block"> *@
@*                       <span style="float:left;">@bets.ElementAt(i).betsList.ElementAt(j).betName *@
@*                       </span><span style="float:right;"><b>@bets.ElementAt(i).betsList.ElementAt(j).oddValue</b></span> *@
@*                   </button> *@
@*               </div> *@
@*           } *@
@*         </div>                                   *@
@*     } *@
@* </div> *@
@* *@
