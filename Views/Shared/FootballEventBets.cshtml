@* @model dynamic *@
@* *@
@* @using TET_BET.Models *@
@* @{ *@
@*     ViewBag.Title = "title"; *@
@*     Layout = "_Layout"; *@
@*     var footballEvent = ViewData["football_event"] as DBFootballEvent; *@
@*     var bets = ViewData["bets_list"] as List<DBBetType>; *@
@* } *@
@* *@
@* <div class="text-center"> *@
@*     <h1>@footballEvent.footballTeam0.footballTeamName - @footballEvent.footballTeam1.footballTeamName</h1> *@
